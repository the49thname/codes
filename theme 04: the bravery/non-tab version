<!-- sticky navbar jquery -->

{block:ifsmallheader}
<script>
$(document).ready(function(){ 
$(window).bind('scroll', function(){
 if($(window).scrollTop() > 250){
 $('.nav').addClass('nav-fixed');
 }

 else{
 $('.nav').removeClass('nav-fixed');
 }
 });
});
</script>
{/block:ifsmallheader}

{block:ifnotsmallheader}
<script>
$(document).ready(function(){ 
$(window).bind('scroll', function(){
 if($(window).scrollTop() > 400){
 $('.nav').addClass('nav-fixed');
 }

 else{
 $('.nav').removeClass('nav-fixed');
 }
 });
});
</script>
