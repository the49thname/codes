<!-- sticky navbar jquery -->

<script>
$(document).ready(function(){ 
$(window).bind('scroll', function(){
 if($(window).scrollTop() > 200){
 $('.nav').addClass('nav-fixed');
 }

 else{
 $('.nav').removeClass('nav-fixed');
 }
 });
});
</script>

