<!-- pxu photosets script -->

<script src="//static.tumblr.com/yxfeliq/hHwojmt8m/bctphotoset.min.js"></script>

<script type="text/javascript">
$(document).ready(function() {
//updates toggle
$('.updatestoggle').click(function(){
$('.updates').slideToggle('slow');
$('.updatestoggle').toggleClass('activated');
$('#sidebarupdate').toggleClass('overflow');
});
//pagination
if ($('body').is('.page1')) {
$('a#back').addClass('greyedout');
$('a#back').removeAttr('title href');
}
//sidebar links
$('#sidebarnav a').each(function () {
if ($(this).is(':empty')) {
$(this).remove();
}});
//pagination
if ($('body').is('.page{TotalPages}')) {
$('a#next').addClass('greyedout');
$('a#next').removeAttr('title href');
}
//pxu photosets
$('.photo-slideshow').pxuPhotoset({
lightbox: true,
rounded: false,
gutter: '2px',
borderRadius: '0',
photoset: '.photo-slideshow',
photoWrap: '.photo-data',
photo: '.pxu-photo'
});
//empty caption fix
$("p:empty").remove();
$(".unnest").each(function () {
if ($(this).is(":empty")){
$(this).parents(".unnestcont").remove();
}});
});
</script>

