<!-- scrollto jquery -->

{block:iffullsizeheader}
<script type="text/javascript">
jQuery(document).ready(function(){
var containerPosition = jQuery('#themecontainer').offset().top;
jQuery('#scroll').click(function(){
jQuery('html, body').animate({scrollTop:containerPosition}, 'slow');
return false;
});
});
</script>
{/block:iffullsizeheader}

<!-- fixed div jquery -->

{block:ifshowheader}
{block:IndexPage}
{block:iffullsizeheader}
<script type="text/javascript">
$(document).ready(function(){
var winH = $(window).height();
var sticky = "sticky";
$(window).scroll(function() {
if( $(window).scrollTop() > winH ) {
$(".leftsidebarcont").addClass(sticky);
$(".rightsidebarcont").addClass(sticky);
} else {
$(".leftsidebarcont").removeClass(sticky);
$(".rightsidebarcont").removeClass(sticky);
}
});
});
</script>
{/block:iffullsizeheader}
{block:ifnotfullsizeheader}
<script type="text/javascript">
$(document).ready(function(){
var sticky = "sticky";
$(window).scroll(function() {
if( $(window).scrollTop() > 300 ) {
$(".leftsidebarcont").addClass(sticky);
$(".rightsidebarcont").addClass(sticky);
} else {
$(".leftsidebarcont").removeClass(sticky);
$(".rightsidebarcont").removeClass(sticky);
}
});
});
</script>
{/block:ifnotfullsizeheader}
{/block:IndexPage}
{/block:ifshowheader}

