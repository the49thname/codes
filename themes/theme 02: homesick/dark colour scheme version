<!-- jquery -->

<script type="text/javascript">
$(document).ready(function(){
//tabs
{block:ifshowalltabs}
$('.tabs .tab-links a').on('click', function(e) {
var currentAttrValue = $(this).attr('href');
$('.tabs ' + currentAttrValue).fadeIn(400).siblings().hide();
$(this).parent('li').addClass('active').siblings().removeClass('active');
e.preventDefault();
});
//hide ask tab on /ask page
{block:ifshowasktab}
$(function(){
if (window.location.pathname == "/ask") {
$('#tab2').remove();
$('.asklink').replaceWith('<li id="asklink"><a href="/ask">Ask</a></li>');
}
});
{block:ifshowfaq}
//faq toggles
$('.faqq').click(function() {
$('#faq' + $(this).attr('target')).slideToggle(400);
});
// remove empty faqs
$(".faqq h1").each(function() {
if ($(this).is(":empty")){
$(this).parents(".faq").remove();
}});
{/block:ifshowfaq}
{/block:ifshowasktab}
{block:ifshowabouttab}
$("#profile li span").each(function() {
if ($(this).is(":empty")){
$(this).parents("#profile li").remove();
}});
{/block:ifshowabouttab}
{/block:ifshowalltabs}
});
</script>

