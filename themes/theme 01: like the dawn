<script type="text/javascript">
$(document).ready(function(){
//masonry
{block:IndexPage}
var $container = $('#postcontainer');
$container.masonry({ itemSelector: '.posts' });
$container.imagesLoaded(function(){
$container.masonry();
$container.find('.posts').animate({ opacity: 1, zIndex: 1 });
});
{/block:IndexPage}
});
</script>

